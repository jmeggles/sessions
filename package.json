{
  "name": "react-jwt",
  "version": "1.0.0",
  "description": "A boiler plate application using React, Express, Node and Mongo with JSON Web Tokens for authentication",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "install": "cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaab30/react-jwt-boiler-plate.git"
  },
  "author": "Jorge Alvarez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jaab30/react-jwt-boiler-plate/issues"
  },
  "homepage": "https://github.com/jaab30/react-jwt-boiler-plate#readme",
  "dependencies": {
    "aws-sdk": "^2.738.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.20",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "path": "^0.12.7",
    "react": "^16.13.1",
    "react-avatar-edit": "^0.8.3",
    "react-avatar-editor": "^11.0.10",
    "react-dropzone": "^11.0.3",
    "react-toastify": "^6.0.8",
    "superagent": "^6.0.0",
    "url": "^0.11.0",
    "validator": "^13.1.1"
  }
}
